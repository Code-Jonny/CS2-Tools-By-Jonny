{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "CS2ToolsByJonny",
  "version": "1.0.0",
  "identifier": "com.jonny.cs2tools",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "CS2 Tools by Jonny",
        "width": 1100,
        "height": 800,
        "minHeight": 540,
        "minWidth": 600,
        "maxWidth": 1300,
        "maxHeight": 900,
        "resizable": true,
        "maximizable": false,
        "fullscreen": false,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["nsis"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
