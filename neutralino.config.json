{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "cs2tools.app",
  "applicationName": "CS2 Tools by Jonny",
  "author": "Jonny",
  "description": "A set of tools for a better Counter-Strike 2 experience by Jonny",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/app/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "exportAuthInfo": false,
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log",
    "storage.*",
    "filesystem.*",
    "window.*"
  ],
  "modes": {
    "window": {
      "title": "CS2 Tools by Jonny",
      "icon": "/app/icon.png",
      "width": 1100,
      "height": 800,
      "minWidth": 1100,
      "minHeight": 800,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true,
      "injectGlobals": true
    }
  },
  "cli": {
    "binaryName": "cs2tools",
    "resourcesPath": "/app/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "6.1.0",
    "clientVersion": "5.5.0"
  }
}
